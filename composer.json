{
    "name": "kdesterik/morencykel",
    "description": "MorenCykel: E-commerce platform",
    "license": "MIT",
    "authors": [{
        "name": "Koen Dirk van Esterik",
        "email": "kdesterik@gmail.com"
    }],

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "elliotcondon/acf",
                "version": "4.4.2",
                "type": "wordpress-plugin",
                "source": {
                    "url": "https://github.com/elliotcondon/acf.git",
                    "type": "git",
                    "reference": "4.4.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "woothemes/woocommerce",
                "version": "2.4.4",
                "type": "wordpress-plugin",
                "source": {
                    "url": "https://github.com/woothemes/woocommerce.git",
                    "type": "git",
                    "reference": "2.4.4"
                }
            }
        }
    ],

    "require": {
        "composer/installers": "~1.0.0",
        "elliotcondon/acf": "4.4.2",
        "woothemes/woocommerce": "2.4.4"
    },

    "extra": {
        "installer-paths": {
            "./dist/wp-content/plugins/{$name}": [ 
                "elliotcondon/acf",
                "woothemes/woocommerce"
            ]
        }
    }
}
